# Environment variables for the Twitch bot server
# Copy this file to .env and fill in the values

# Convex deployment URL
CONVEX_URL=

# Secret key for API authentication (generate a random string)
BOT_API_SECRET=

# Twitch OAuth credentials (same as main app)
TWITCH_CLIENT_ID=
TWITCH_CLIENT_SECRET=

# Server port (optional, defaults to 3002)
PORT=3002
